<!--pages/commodity/commodity.wxml-->
<van-sticky>
  <van-search value="{{ value }}" shape="round" placeholder="输入型号或规格" bind:focus="onSearch" />
</van-sticky>
<van-tree-select items="{{ items }}" main-active-index="{{ mainActiveIndex }}" active-id="{{ activeId }}"
  max="{{ max }}" bind:click-nav="onClickNav" bind:click-item="onClickItem" />